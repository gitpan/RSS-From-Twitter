#!perl

our $VERSION = '0.04'; # VERSION

use 5.010;
use strict;
use warnings;

use Perinci::CmdLine::Any;
Perinci::CmdLine::Any->new(
    url => '/RSS/From/Twitter/get_rss_from_twitter_list')->run;

1;
#ABSTRACT: Convert Twitter public list page to RSS
#PODNAME: get-rss-from-twitter-search

__END__

